#include <bits/stdc++.h>
using namespace std;

int main() {
    int numCourses;
    cin >> numCourses;

    int m;
    cin >> m; // number of prerequisite pairs

    vector<vector<int>> adj(numCourses);
    vector<int> indegree(numCourses, 0);

    for (int i = 0; i < m; i++) {
        int a, b;
        cin >> a >> b;
        adj[b].push_back(a);
        indegree[a]++;
    }

    queue<int> q;
    for (int i = 0; i < numCourses; i++) {
        if (indegree[i] == 0)
            q.push(i);
    }

    int completed = 0;

    while (!q.empty()) {
        int course = q.front();
        q.pop();
        completed++;

        for (int next : adj[course]) {
            indegree[next]--;
            if (indegree[next] == 0)
                q.push(next);
        }
    }

    if (completed == numCourses)
        cout << "true";
    else
        cout << "false";

    return 0;
}

